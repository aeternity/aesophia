include "List.aes"

contract ListComp =

    entrypoint sample1() = [1,2,3]
    entrypoint sample2() = [4,5]

    entrypoint l1() = [x | x <- sample1()]
    entrypoint l1_true() = [1,2,3]

    entrypoint l2() = [x + y | x <- sample1(), y <- sample2()]
    entrypoint l2_true() = [5,6,6,7,7,8]

    entrypoint l3() = [x ++ y | x <- [[":)"] | x <- [1,2]]
                              , y <- [[":("]]]  
    entrypoint l3_true() = [[":)", ":("], [":)", ":("]]
